<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="E2E Suite">
    <listeners>
    <listener class-name="com.ayush.automation.listeners.ExtentListener"/>
        <listener class-name="com.ayush.automation.listeners.TestListener"/>
        <listener class-name="com.ayush.automation.retry.RetryListener"/>
    </listeners>

    <test name="End to End Flow">
        <classes>
            <class name="com.ayush.automation.tests.LoginTest">
                <methods>
                    <include name="endToEndCheckoutFlow"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
